/* pages/indent/indent.wxss */
.identHeader{
  height: 200rpx;
  background: #1BA8FC;
  padding:10rpx 5%;
  line-height: 200rpx;
}
.identWhite{
    color: #fff;
    margin-left: 10rpx;
    position: absolute;
    top: 0;
}
.identSection {
  height: 400rpx;
  background: #fff;
  padding: 20rpx 5%;
}
.identSection text {
  font-size: 28rpx;
}
.identSection view {
  line-height: 80rpx;
  overflow: hidden;
}

.identSection image {
  display: block;
  float: left;
  width: 40rpx;
  height: 40rpx;
  margin-right: 10rpx;
  margin-top: 20rpx;
}
.orange {
  color: #f66000;
}
.identBor{
   padding-bottom: 20rpx;
   border-bottom: 0.005ex solid #e0e0e0;
}
.identBottom{
    font-size: 28rpx;
    height: 60rpx;
    line-height: 60rpx;
}
